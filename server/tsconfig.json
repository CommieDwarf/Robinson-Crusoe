{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es6"
    ],
    "paths": {
      "@shared/*": [
        "./src/shared/*"
      ]
    },
    "allowJs": true,
    "outDir": "build",
    "rootDir": "src",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react"
  },
  "exclude": [
    "node_modules"
  ]
}
